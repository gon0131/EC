<%= render "layouts/header_admin" %>
<div class="container">
  <h3>ジャンル編集</h3>
  <%= form_with model: @genre, url: admin_genre_path(@genre.id), local: true do |f| %>
    <%= f.label "ジャンル名" %>
    <%= f.text_field :name %>
    <%= f.submit "変更を保存", class:"btn btn-success text-light" %>
  <% end %>
</div>